AHA;QAGA;QACAC,aAJA,CAIA;;MAJA,CAFA;MAQA;MACAC;QACAd;UAAAe;UAAAC;UAAAC;QAAA,GACA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA,CADA,EACA;UACAA,eADA;UAEAG,0BAFA,CAEA;;QAFA,CADA,CADA;QAMAT;UAAAI;UAAAC;UAAAC;QAAA,GACA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA,CADA,EACA;UACAA,eADA;UAEAG;QAFA,CADA,CANA;QAWAR;UAAAG;UAAAC;UAAAC;QAAA,EAXA;QAYAJ;UAAAE;UAAAC;UAAAC;QAAA,GACA;UAAAA;UAAAC;UAAAC;UAAAH;QAAA,CADA;MAZA,CATA;MAwBAK;IAxBA;EA0BA,CA7DA;EA8DAC;IACAC,SADA,uBACA;MACA;IACA;EAHA,CA9DA;EAmEAC;IACAC,iBADA,+BACA;MAAA;;MAAA;QAAA;UAAA;YAAA;cAAA;gBAAA;gBAAA,OACAA,oBADA;;cAAA;gBACA,cADA;;cAAA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAEA,CAHA;IAIAC,eAJA,2BAIAC,EAJA,EAIA;MAAA;;MAAA;QAAA;UAAA;YAAA;cAAA;gBAAA;gBAAA,OACAD,oBADA;;cAAA;gBACA,eADA;;cAAA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAEA,CANA;IAOAE,KAPA,mBAOA;MAAA;;MACA;QAAA;UAAA;YAAA;cAAA;gBAAA;kBAAA,KACAC,IADA;oBAAA;oBAAA;kBAAA;;kBAAA,KAEA,kBAFA;oBAAA;oBAAA;kBAAA;;kBAAA;kBAAA,OAGAC,kCAHA;;gBAAA;kBAAA;kBAAA;;gBAAA;kBAAA;kBAAA,OAKAC;oBAAAC;kBAAA,GALA;;gBAAA;kBAQA;;kBACA;;gBATA;gBAAA;kBAAA;cAAA;YAAA;UAAA;QAAA;;QAAA;UAAA;QAAA;MAAA;IAWA,CAnBA;IAoBAC,SApBA,uBAoBA;MACA;QACAjC,QADA;QACA;QACAW,QAFA;QAEA;QACAC,WAHA;QAGA;QACAC;MAJA;MAMA;MACA;IACA;EA7BA;AAnEA","names":["name","props","showDialog","type","default","treeNode","data","getDepartments","depts","isRepeat","formData","code","manager","introduce","rules","required","message","trigger","min","max","validator","peoples","computed","showTitle","methods","getEmployeeSimple","getDepartDetail","id","btnOk","vaid","updateDepartments","addDepartments","pid","btnCancel"],"sourceRoot":"src/views/departments/components","sources":["add-dept.vue"],"sourcesContent":["<template>\r\n  <!-- 新增部门的弹层 -->\r\n  <el-dialog :title=\"showTitle\" :visible=\"showDialog\" close-on-click-modal @close=\"btnCancel\">\r\n    <!-- 表单组件  el-form   label-width设置label的宽度   -->\r\n    <!-- 匿名插槽 -->\r\n    <el-form ref=\"deptForm\" :model=\"formData\" :rules=\"rules\" label-width=\"120px\">\r\n      <el-form-item label=\"部门名称\" prop=\"name\">\r\n        <el-input v-model=\"formData.name\" style=\"width:80%\" placeholder=\"1-50个字符\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"部门编码\" prop=\"code\">\r\n        <el-input v-model=\"formData.code\" style=\"width:80%\" placeholder=\"1-50个字符\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"部门负责人\" prop=\"manager\">\r\n        <el-select v-model=\"formData.manager\" style=\"width:80%\" placeholder=\"请选择\" @focus=\"getEmployeeSimple\">\r\n          <el-option v-for=\"item in peoples\" :key=\"item.id\" :label=\"item.username\" :value=\"item.username\" />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"部门介绍\" prop=\"introduce\">\r\n        <el-input v-model=\"formData.introduce\" style=\"width:80%\" placeholder=\"1-300个字符\" type=\"textarea\" :rows=\"3\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <!-- el-dialog有专门放置底部操作栏的 插槽  具名插槽 -->\r\n    <el-row slot=\"footer\" type=\"flex\" justify=\"center\">\r\n      <!-- 列被分为24 -->\r\n      <el-col :span=\"6\">\r\n        <el-button type=\"primary\" size=\"small\" @click=\"btnOk\">确定</el-button>\r\n        <el-button size=\"small\" @click=\"btnCancel\">取消</el-button>\r\n      </el-col>\r\n    </el-row>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { addDepartments, getDepartDetail, getDepartments, updateDepartments } from '@/api/departments'\r\nimport { getEmployeeSimple } from '@/api/employees'\r\n\r\nexport default {\r\n  name: 'AddDept',\r\n  props: {\r\n    showDialog: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    treeNode: {\r\n      type: Object,\r\n      default: null\r\n    }\r\n  },\r\n  data() {\r\n    const checkNameRepeat = async (rule, value, callback) => {\r\n      const { depts } = await getDepartments()\r\n      let isRepeat = false\r\n      if (this.formData.id) {\r\n        depts.filter(item => item.pid === this.treeNode.id && item.id !== this.treeNode.id).some(item => item.name === value)\r\n      } else {\r\n        isRepeat = depts.filter(item => item.pid === this.treeNode.id).some(item =>item.name ===value)\r\n      }\r\n\r\n      isRepeat ? callback(new Error(`同级部门已经存在这个${value}部门`)) : callback()\r\n    }\r\n    const checkCodeRepeat = async(rule, value, callback) => {\r\n      // 先要获取最新的组织架构数据\r\n      const { depts } = await getDepartments()\r\n      let isRepeat = false\r\n      if (this.formData.id) {\r\n        isRepeat = depts.filter(item => item.pid !== this.treeNode.id).some(item => item.code === value && value)\r\n      } else {\r\n        isRepeat = depts.some(item => item.code === value && value) // 这里加一个 value不为空 因为我们的部门有可能没有code\r\n      }\r\n      isRepeat ? callback(new Error(`组织架构中已经有部门使用${value}编码`)) : callback()\r\n    }\r\n    return {\r\n      // 定义表单数据\r\n      formData: {\r\n        name: '', // 部门名称\r\n        code: '', // 部门编码\r\n        manager: '', // 部门管理者\r\n        introduce: '' // 部门介绍\r\n      },\r\n      // 定义校验规则\r\n      rules: {\r\n        name: [{ required: true, message: '部门名称不能为空', trigger: 'blur' },\r\n          { min: 1, max: 50, message: '部门名称要求1-50个字符', trigger: 'blur' }, {\r\n            trigger: 'blur',\r\n            validator: checkNameRepeat // 自定义函数的形式校验\r\n          }],\r\n        code: [{ required: true, message: '部门编码不能为空', trigger: 'blur' },\r\n          { min: 1, max: 50, message: '部门编码要求1-50个字符', trigger: 'blur' }, {\r\n            trigger: 'blur',\r\n            validator: checkCodeRepeat\r\n          }],\r\n        manager: [{ required: true, message: '部门负责人不能为空', trigger: 'blur' }],\r\n        introduce: [{ required: true, message: '部门介绍不能为空', trigger: 'blur' },\r\n          { trigger: 'blur', min: 1, max: 300, message: '部门介绍要求1-50个字符' }]\r\n      },\r\n      peoples: []\r\n    }\r\n  },\r\n  computed: {\r\n    showTitle() {\r\n      return this.formData.id ? '编辑部门' : '新增子部门'\r\n    }\r\n  },\r\n  methods: {\r\n    async getEmployeeSimple() {\r\n      this.peoples = await getEmployeeSimple()\r\n    },\r\n    async getDepartDetail(id) {\r\n      this.formData = await getDepartDetail(id)\r\n    },\r\n    btnOk() {\r\n      this.$refs.deptForm.validate(async (vaid) => {\r\n        if(vaid) {\r\n          if(this.formData.id) {\r\n            await updateDepartments(this.formData)\r\n          } else {\r\n            await addDepartments({ ...formData, pid: this.treeNode.id })\r\n          }\r\n        }\r\n        this.$emit('addDepts')\r\n        this.$emit('update:showDialog', false)\r\n      })\r\n    },\r\n    btnCancel() {\r\n      this.formData = {\r\n        name: '', // 部门名称\r\n        code: '', // 部门编码\r\n        manager: '', // 部门管理者\r\n        introduce: ''\r\n      }\r\n      this.$refs.deptForm.resetFields()\r\n      this.$emit('update:showDialog', false)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}